{
  "name": "api-scheduler",
  "version": "1.0.0",
  "description": "API Scheduler",
  "main": "dist/server.js",
  "scripts": {
    "watch": "tsc -w -p ./",
    "debug": "nodemon ./dist/server.js",
    "docker-debug": "docker-compose up",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --ext=.ts src/",
    "build": "npx tsc && copyfiles -u 1 src/app/**/*.html src/app/**/*.css dist/",
    "start": "node ./dist/server.js",
    "start-dev": "npm run build && cross-env NODE_ENV=development node ./dist/server.js",
    "start-alpha": "npm run build && cross-env NODE_ENV=alpha node ./dist/server.js",
    "start-secure": "npm run build && cross-env NODE_ENV=secure node ./dist/server.js"
  },
  "author": "Vignesh T.V.",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.18.2",
    "chalk": "^2.3.0",
    "express": "^4.16.2",
    "glob": "^7.1.2",
    "helmet": "^3.9.0",
    "jaeger-client": "^3.14.4",
    "lodash": "^4.17.19",
    "method-override": "^2.3.10",
    "moment": "^2.29.4",
    "mongoose": "^5.4.15",
    "morgan": "^1.9.1",
    "node-rsa": "^1.0.0",
    "node-schedule": "^1.3.0",
    "opentracing": "^0.14.3",
    "prom-client": "^11.2.1",
    "pug": "^2.0.3",
    "winston": "^2.4.0",
    "xml": "^1.0.1",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/express": "^4.11.0",
    "@types/node": "^9.4.6",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "@typescript-eslint/typescript-estree": "^1.4.2",
    "copyfiles": "^2.0.0",
    "cross-env": "^5.1.3",
    "eslint": "^5.15.1",
    "eslint-config-google": "^0.9.1",
    "typescript": "^3.3.3333"
  }
}
